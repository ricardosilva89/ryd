{
	"compilerOptions": {
		"target": "es2022",
		"module": "commonjs",
		"lib": ["ES2022"],
		"esModuleInterop": true,
		"outDir": "./dist",
		"rootDir": "src",
		"allowJs": false,
		"sourceMap": true,
		"sourceRoot": "./src",
		"typeRoots": ["./node_modules/@types"],
		"types": ["jest"],
		"resolveJsonModule": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["./node_modules", "./dist", "./data"]
}
